
# test video
raspivid -w 640 -h 480 -p 100,100,640,480

# test with effects integrated ein the driver
# gray level with direct preview and h264 output
raspivid -w 640 -h 480 -p 100,100,640,480 --colfx 128:128

# gray level with direct preview and yuv output
raspividyuv -w 640 -h 480 -p 100,100,640,480 --colfx 128:128

# get yuv420 output
raspividyuv -n -w 640 -h 480 -o - | ./viewer -c -w 640*480
raspividyuv -n -w 640 -h 480 -o - | ./viewer -w 640*480




